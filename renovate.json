{
  "extends": ["github>gemunion/renovate-config"],
  "baseBranches": ["master", "mui5"],
  "timezone": "Asia/Singapore",
  "schedule": [
    "after 4am",
    "before 5am"
  ],
  "packageRules": [
    {
      "updateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "matchPackagePatterns": ["^@material-ui/"],
      "rollbackPrs": false
    },
    {
      "matchPackagePatterns": ["notistack"],
      "rollbackPrs": false
    }
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "npmrc": "@gemunion:registry=https://npm.pkg.github.com/"
}
